                "9": {
                  "title": "Cookies",
                  "id": 111,
                  "ideas": {
                    "1": {
                      "title": "Cookie Basics",
                      "id": 195,
                      "ideas": {
                        "1": {
                          "title": "client side state control",
                          "id": 236
                        },
                        "2": {
                          "title": "Cookies are just names with associated contents",
                          "id": 237
                        },
                        "3": {
                          "title": "The cookies are sent to the client by the server.",
                          "id": 238
                        },
                        "4": {
                          "title": "The server tells the client for what path and host name it wants the cookie sent back, and it also sends an expiration data and a few more properties.",
                          "id": 239
                        },
                        "5": {
                          "title": "When a client communicates with a server with a name and path as previously specified in a received cookit, the client sends back the cookies and their contents to the server, unless of course they are expired.",
                          "id": 240
                        },
                        "6": {
                          "title": "Many applications and servers use this method to connect a series of requests into a single logical session.",
                          "id": 241
                        }
                      },
                      "attr": {
                        "collapsed": true,
                        "style": {}
